{
  "name": "maintenance-app",
  "version": "25.1.30",
  "description": "DHIS2 Maintenance app",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/maintenance-app.git"
  },
  "license": "BSD-3-Clause",
  "scripts": {
    "prestart": "d2-manifest package.json manifest.webapp",
    "start": "webpack-dev-server",
    "test": "echo Everything probably works great\\! ## karma start test/config/karma.config.js --single-run true",
    "build": "rm -rf build && NODE_ENV=production webpack && npm run manifest",
    "postbuild": "cp -r src/i18n icon.png node_modules/babel-polyfill/dist/polyfill.min.js node_modules/ckeditor ./build/",
    "validate": "npm ls --depth 0",
    "manifest": "d2-manifest package.json build/manifest.webapp",
    "deploy": "npm run build && mvn clean deploy",
    "lint": "echo Looks good."
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-core": "^6.13.2",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-runtime": "^6.12.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "d2": "^25.2.0",
    "d2-manifest": "^1.0.0-2",
    "d2-ui": "^25.5.1",
    "d2-utilizr": "^0.2.9",
    "d3-color": "^1.0.2",
    "d3-format": "^1.0.2",
    "d3-scale": "^1.0.3",
    "fbjs": "^0.2.1",
    "html-webpack-plugin": "^2.22.0",
    "lodash.isfinite": "^3.3.1",
    "lodash.isnumber": "^3.0.3",
    "loglevel": "^1.4.0",
    "material-ui": "^0.15.4",
    "moment": "^2.16.0",
    "node-pre-gyp": "^0.6.30",
    "node-sass": "^3.7.0",
    "precommit-hook": "^3.0.0",
    "react": "^15.3.1",
    "react-addons-create-fragment": "^15.3.1",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-linked-state-mixin": "^15.3.1",
    "react-addons-perf": "^15.3.1",
    "react-addons-shallow-compare": "^15.3.1",
    "react-addons-test-utils": "^15.3.1",
    "react-color": "^2.2.7",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.3.1",
    "react-router": "^2.6.1",
    "react-tap-event-plugin": "^1.0.0",
    "recompose": "^0.20.2",
    "rx": "^4.1.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "ckeditor": "^4.5.9"
  },
  "pre-commit": [
    "validate"
  ],
  "manifest.webapp": {
    "name": "Maintenance app",
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "",
      "name": "Mark Polak"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  }
}
