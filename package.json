    "ignore-styles": "^5.0.1",
    "jest": "^23.0.1",
    "jest-enzyme": "^4.0.0",
    "json-loader": "^0.5.4",
    "lodash.isfinite": "^3.3.1",
    "lodash.isnumber": "^3.0.3",
    "loglevel": "^1.4.0",
    "moment": "^2.16.0",
    "node-fetch": "^1.6.3",
    "node-pre-gyp": "^0.6.30",
