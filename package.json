    "react-dom": "^16.4.2",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.3",
    "react-router": "^4.3.1",
    "react-sortable-hoc": "^0.8.3",
    "react-speed-dial": "^0.5.2",
    "react-tap-event-plugin": "^3.0.3",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.0",
    "redux-observable": "1.0.0",
    "rxjs": "^6.2.2"
  },
  "pre-commit": [
    "validate",
